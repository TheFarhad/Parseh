<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Color x:Key="BackgroundVeryLightColor" >#fafafa</Color>
    <SolidColorBrush Color="{StaticResource BackgroundVeryLightColor}" x:Key="BackgroundVeryLightBrush"/>

    <Color x:Key="BackgroundLightColor" >#efefef</Color>
    <SolidColorBrush Color="{StaticResource BackgroundLightColor}" x:Key="BackgroundLightBrush"/>

    <Color x:Key="ForegroundMainColor" >#686868</Color>
